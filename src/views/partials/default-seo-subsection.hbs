<hr class="section-line">
<div class="item py-3" id="{{this.key}}">
    <div class="item-header">
        <div class="item-title">
            <div style="padding-left: 2%;  width: 100%;">
                <h3>{{this.friendlyName}}</h3>
                <div style="margin-left: 2%;margin-top: 4px;" class="item-num {{this.passedClass}}">
                    <p>{{this.circleTextDisplay}}</p>
                </div>
            </div>
            <div>
                <div class="item-section">{{this.section}}</div>
                {{!-- <img data-title="Title Here" data-info="Info Here" class="question" src="img/info.png" alt="Info"> --}}
            </div>
        </div>
    </div>
    <div class="item-body myt-2">
        {{#if this.passed}}
        <div class="box-wrapper">
            {{#if this.shortAnswer}}
            <div class="answer">
                <div><img style="height: 50px;" src="img/success.png" alt="Check"></div>
                <div style="margin-top: 20px;">
                    <p>
                        {{this.shortAnswer}}
                    </p>
                </div>
            </div>
            {{/if}}
            {{#if this.value}}
            <div class="answer">
                <div><img style="height: 50px;" src="img/success.png" alt="Check"></div>
                <div style="margin-top: 20px;">
                    <p>
                        Value: <br> {{this.value}}
                    </p>
                </div>
            </div>
            {{/if}}
        </div>
        {{else}}
        {{#if this.shortAnswer}}
        <div class="box-wrapper">
            <div class="answer">
                <div><img style="height: 50px;" src="img/err.png" alt="Check"></div>
                <div style="margin-top: 20px;">
                    <p>
                        {{this.shortAnswer}}
                    </p>
                </div>
            </div>
        </div>
        {{/if}}
        {{#if this.value}}
        <div class="answer">
            <div><img style="height: 50px;" src="img/info3.png" alt="Value"></div>
            <div style="margin-top: 20px;">
                <p>
                    Value: <br> {{this.value}}
                </p>
            </div>
        </div>
        {{/if}}
        {{#if this.recommendation}}
        <div class="box-wrapper">
            <div class="danger">
                <div><img style="height: 50px;" src="img/info3.png" alt="recommendation"></div>
                <div style="margin-top: 20px;">
                    <p>
                        Recommendation: <br> {{this.recommendation}}
                    </p>
                </div>
            </div>
        </div>
        {{/if}}

        {{/if}}

        {{#if this.data}}
        {{#ifObject this.data}}
        <div class="answer" style="margin-top: 20px;">

            {{#ifeq this.key 'backlinksList'}}
            <ul>
                {{#each this.data}}
                {{#ifArray this}}

                {{else}}
                <li>
                    {{friendlyText @key}}: {{this}}
                </li>
                {{/ifArray}}
                {{/each}}
            </ul>
            {{else}}
            <div><img style="height: 50px;" src="img/info3.png" alt="data"></div>
            <div>


                <ul>
                    {{#each this.data}}
                    {{#ifArray this}}

                    {{else}}
                    <li>
                        {{friendlyText @key}}: {{this}}
                    </li>
                    {{/ifArray}}
                    {{/each}}
                </ul>
            </div>
            {{/ifeq}}

        </div>

        {{else}}
        <div class="answer">
            <div><img style="height: 50px;" src="img/info3.png" alt="data"></div>
            <div style="margin-top: 20px;">
                {{this.data}}
            </div>
        </div>
        {{/ifObject}}
        {{/if}}

        {{#if this.data}}
        {{#each this.data}}
        {{#ifArray this}}
        {{> table-subsection this}}
        {{/ifArray}}
        {{#ifObject this}}
        {{> table-subsection this}}
        {{/ifObject}}
        {{/each}}
        {{/if}}
    </div>
</div>