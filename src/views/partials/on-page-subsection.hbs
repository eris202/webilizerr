<hr class="section-line">
<div class="item py-3" id="{{this.key}}">
    <div class="item-header">
        <div class="item-num {{this.passedClass}}">
            <p>{{this.circleTextDisplay}}</p>
        </div>
        <div class="item-title">
            <h3>{{this.friendlyName}}</h3>
            <div>
                <div class="item-section">Seo</div>
                <img data-title="Title Here" data-info="Info Here" class="question" src="img/info.png"
                    alt="Info">
            </div>
        </div>
    </div>
    <div class="item-body myt-2">
        <div class="box-wrapper">
            {{#if this.shortAnswer}}
            <div class="answer">
                <div><img src="img/success.png" alt="Check"></div>
                <div>
                    <p>
                        {{this.shortAnswer}}
                    </p>
                </div>
            </div>
            {{/if}}
        </div>
        <div class="box-wrapper">
            <div class="info">
                <div><img src="img/info2.png" alt="info"></div>
                <div class="table-wrapper">
                    <table cellpadding="0" cellspacing="0" border="0" class="table header-tags">
                        <thead>
                            <tr>
                                <th>Total</th>
                                <th>Files Count</th>
                                <th>External Count</th>
                                <th>No follow count</th>
                                <th>External Percentage</th>
                                <th>No follow percentage</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>{{#numeric this.data.total}}{{/numeric}}</td>
                                <td>{{#numeric this.data.filesCount}}{{/numeric}}</td>
                                <td>{{#numeric this.data.externalCount}}{{/numeric}}</td>
                                <td>{{#numeric this.data.noFollowCount}}{{/numeric}}</td>
                                <td>{{#numeric this.data.externalPercentage}}{{/numeric}}</td>
                                <td>{{#numeric this.data.noFollowPercentage}}{{/numeric}}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>