<hr class="section-line">
<div class="item py-3" id="fdas">
    <div class="item-header">
        <div class="item-num dfsa">
            <p>{{this.section}}</p>
        </div>
        <div class="item-title">
            <h3>Technologies Found</h3>
            <div>
                <div class="item-section">{{this.section}}</div>
                {{!-- <img data-title="Title Here" data-info="Info Here" class="question" src="img/info.png" alt="Info"> --}}
            </div>
        </div>
    </div>
    <div class="item-body myt-2">
        <div class="box-wrapper">
            <div class="info">
                <div><img style="height: 50px;" src="img/info2.png" alt="recommendation"></div>
                <div style="margin-top: 20px;">
                    <table cellpadding="0" cellspacing="0" border="0" class="table header-tags">
                        <thead>
                            <tr>
                                <th>Technology</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each this.data}}
                            <tr>
                                <td>{{this}}</td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>