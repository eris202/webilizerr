<hr class="section-line">
<div class="item py-3" id="{{this.key}}">
    <div class="item-header">
        <div class="item-num {{this.passedClass}}">
            <p>{{this.circleTextDisplay}}</p>
        </div>
        <div class="item-title">
            <h3>{{this.friendlyName}}</h3>
            <div>
                <div class="item-section">Seo</div>
                <img data-title="Title Here" data-info="Info Here" class="question" src="img/info.png"
                    alt="Info">
            </div>
        </div>
    </div>
    <div class="item-body myt-2">
        <div class="box-wrapper">
            <div class="answer">
                <div><img src="img/success.png" alt="Check"></div>
                <div>
                    <p>
                        {{this.shortAnswer}}
                    </p>
                </div>
            </div>
        </div>
        <div class="box-wrapper">
            <div class="info">
                <div><img src="img/info2.png" alt="info"></div>
                <div class="table-wrapper">
                    <table cellpadding="0" cellspacing="0" border="0" class="table header-tags">
                        <thead>
                            <tr>
                                <th>Header Tag</th>
                                <th>Frequency</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>H1</td>
                                <td>
                                    <div class="bar-count-wrapper">
                                        {{this.data.h1.length}}
                                        <div style="width: 5%;" class="f-bar"></div>
                                    </div>

                                </td>
                            </tr>
                            <tr>
                                <td>H2</td>
                                <td>
                                    <div class="bar-count-wrapper">
                                        {{this.data.h2.length}}
                                        <div style="width: 25%;" class="f-bar"></div>
                                    </div>

                                </td>
                            </tr>
                            <tr>
                                <td>H3
                                </td>
                                <td>
                                    <div class="bar-count-wrapper">
                                        {{this.data.h3.length}}
                                        <div style="width: 25%;" class="f-bar"></div>
                                    </div>

                                </td>
                            </tr>
                            <tr>
                                <td>H4</td>
                                <td>
                                    <div class="bar-count-wrapper">
                                        {{this.data.h4.length}}
                                        <div style="width: 30%;" class="f-bar"></div>
                                    </div>

                                </td>
                            </tr>
                            <tr>
                                <td>H5</td>
                                <td>
                                    <div class="bar-count-wrapper">
                                        {{this.data.h5.length}}
                                        <div style="width: 15%;" class="f-bar"></div>
                                    </div>

                                </td>
                            </tr>
                            <tr>
                                <td>H6</td>
                                <td>
                                    <div class="bar-count-wrapper">
                                        {{this.data.h6.length}}
                                        <div style="width: 0%;" class="f-bar"></div>
                                    </div>

                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>